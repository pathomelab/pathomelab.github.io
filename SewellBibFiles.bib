@Article{furumoto1967mathematical,
  author   = {Warren A. Furumoto and Ray Mickey},
  journal  = {Virology},
  title    = {A mathematical model for the infectivity-dilution curve of tobacco mosaic virus: Theoretical considerations},
  year     = {1967},
  issn     = {0042-6822},
  number   = {2},
  pages    = {216-223},
  volume   = {32},
  abstract = {Previous interpretations of infectivity-dilution curves, as obtained with plant viruses, do not account satisfactorily for some experimental observations. A new model based on theoretical considerations was derived to explain the infectivity-dilution curves obtained with tobacco mosaic virus (TMV). While a single viable TMV particle may initiate infection in any epidermal cell of a leaf, each cell is assumed to have an associated probability that a given TMV particle will establish infection; it is assumed that these probabilities are not necessarily the same for all cells, but may vary from cell to cell. A mathematical expression for the infectivity-dilution curve is derived from additional assumptions about the distribution of the cell infection probabilities. An approximation, which is appropriate when a small fraction of the total number of cells develop lesions, is developed as a limiting case of the derived expression. The approximation, which is of the form y = A log (1 + BV), in which y is the expected number of lesions per half leaf, V is virus concentration, and A and B are constants, is valid under a much broader class of descriptions of the distribution of cell probabilities than that used in deriving the complete equation. In terms of the approximation, the number of lesions is proportional to the concentration at low concentrations, and is proportional to the logarithm of the concentration at high concentrations. Comparison of experimental data with the developed equations is made in a succeeding paper (Furumoto and Mickey, 1967), in which biological implications of the model are discussed.},
  doi      = {https://doi.org/10.1016/0042-6822(67)90271-1},
  url      = {https://www.sciencedirect.com/science/article/pii/0042682267902711},
}

