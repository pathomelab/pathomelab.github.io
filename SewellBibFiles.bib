@Article{Kock2009prevalence,
  author   = {K{\"o}ck, R. and Harlizius, J. and Bressan, N. and Laerberg, R. and Wieler, L. H. and Witte, W. and Deurenberg, R. H. and Voss, A. and Becker, K. and Friedrich, A. W.},
  title    = {Prevalence and molecular characteristics of methicillin-resistant Staphylococcus aureus (MRSA) among pigs on German farms and import of livestock-related MRSA into hospitals},
